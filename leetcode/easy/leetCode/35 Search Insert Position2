#include<iostream>
int main(){
	
}

class Solution {
public:
    int searchInsert(int A[], int n, int target) {
    int i = 0;

    while (target < A[i]) ++i;
    return i + 1;


       	if ( target <= A[0])	{
		return 0;
	    }
    int i;
	for (i = 0; i < n; ++i) {
		if (target >= A[i]) 
		    return i;
		else {	
		}
	}
	return i;
    }
};